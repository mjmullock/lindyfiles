
DROP TABLE IF EXISTS users;
CREATE TABLE users (
    username    VARCHAR(100),
    password    VARCHAR(100),
    fname       VARCHAR(100),
    email       VARCHAR(200),
    picture     VARCHAR(100),
    leader      BOOLEAN,
    follower    BOOLEAN,
    sessid      VARCHAR(100),
    PRIMARY KEY (email)
);

DROP TABLE IF EXISTS pros;
CREATE TABLE pros (
    fname       VARCHAR(100),
    lname       VARCHAR(100),
    leader      BOOLEAN,
    follower    BOOLEAN,
    website     VARCHAR(100),
    teacher     BOOLEAN,
    PRIMARY KEY (fname, lname)
);
